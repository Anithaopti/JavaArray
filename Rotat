public class Rotate
{ void rota(int arr[], int d, int n)
{
for(int i=0; i<d;i++)
{
rot(arr,n);
}
}
void rot(int arr[],int n)
{
int i,temp;
temp=arr[n-1];
for(i=n-1;i>n-1;i--)
array[i]=array[i-1];
array[0]=temp;
}
public static void main(String[] args)
{
arr[]={12,56,87,43,89,90,87};
int r=3, n= arr.length;
rota(arr,r,n);
for(int i=0;i<n;i++)
{
System.out.println(arr[i]);
}
}
}

